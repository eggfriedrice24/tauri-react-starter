  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.5.0+sha512.11106a5916c7406fe4b8cb8e3067974b8728f47308a4f5ac5e850304afa6f57e2847d7950dfe78877d8d36bfb401d381c4215db3a4c3547ffa63c14333a6fa51",
  "description": "A modern, lightweight starter template for building cross-platform desktop applications with Tauri, React, TypeScript, and shadcn/ui",
  "author": "eggfriedrice24",
  "license": "MIT",
  "homepage": "https://github.com/eggfriedrice24/tauri-react-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/eggfriedrice24/tauri-react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/eggfriedrice24/tauri-react-starter/issues"
  },
  "keywords": [
    "tauri",
    "react",
    "typescript",
    "shadcn-ui",
    "tailwindcss",
    "vite",
    "rust",
    "desktop-app",
    "starter-template",
    "boilerplate",
    "cross-platform",
    "eslint",
    "antfu"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "format": "eslint . --fix && cd src-tauri && cargo fmt",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:check": "eslint . && cd src-tauri && cargo fmt --check",
    "format:check:rust": "cd src-tauri && cargo fmt --check"
