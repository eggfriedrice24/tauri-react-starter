{
  "name": "{{PROJECT_NAME}}",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "A modern desktop application built with Tauri, React, and TypeScript",
  "author": "{{AUTHOR}}",
  "license": "MIT",
  "keywords": [
    "tauri",
    "react",
    "typescript",
    "shadcn-ui",
    "tailwindcss",
    "vite",
    "rust",
    "desktop-app",
    "starter-template",
    "boilerplate",
    "cross-platform",
    "eslint",
    "antfu"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "shadcn:add": "pnpm dlx shadcn@latest add",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "format": "eslint . --fix && cd src-tauri && cargo fmt",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:check": "eslint . && cd src-tauri && cargo fmt --check",
    "format:check:rust": "cd src-tauri && cargo fmt --check"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^6.0.3"
  }
}
